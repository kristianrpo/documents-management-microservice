      "id": 6,
      "gridPos": {"h": 5, "w": 4, "x": 12, "y": 8},
      "type": "singlestat",
      "title": "Delete Requests",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "documents_service_delete_requests_total",
          "refId": "A"
        }
      ],
      "format": "none",
