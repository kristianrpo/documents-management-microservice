{
  "uid": "documents-microservice",
  "title": "Documents Microservice - Overview",
  "tags": ["documents", "microservice"],
  "timezone": "browser",
  "schemaVersion": 16,
  "version": 0,
  "refresh": "5s",
  "panels": [
    {
      "id": 1,
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "type": "graph",
      "title": "Request Rate (req/s)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(documents_service_http_requests_total[5m])",
          "legendFormat": "{{method}} {{endpoint}} - {{status}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        {"format": "reqps", "label": null, "logBase": 1, "max": null, "min": 0, "show": true},
        {"format": "short", "label": null, "logBase": 1, "max": null, "min": null, "show": true}
      ]
    },
    {
      "id": 2,
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "type": "graph",
      "title": "Request Duration (avg)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(documents_service_http_request_duration_seconds_sum[5m]) / rate(documents_service_http_request_duration_seconds_count[5m])",
          "legendFormat": "{{method}} {{endpoint}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        {"format": "s", "label": null, "logBase": 1, "max": null, "min": 0, "show": true},
        {"format": "short", "label": null, "logBase": 1, "max": null, "min": null, "show": true}
      ]
    },
    {
      "id": 3,
      "gridPos": {"h": 5, "w": 4, "x": 0, "y": 8},
      "type": "singlestat",
      "title": "Upload Requests",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "documents_service_upload_requests_total",
          "refId": "A"
        }
      ],
      "format": "none",
      "valueName": "current",
      "colors": ["#299c46", "#e0b400", "#bf1b00"],
      "thresholds": ""
    },
    {
      "id": 4,
      "gridPos": {"h": 5, "w": 4, "x": 4, "y": 8},
      "type": "singlestat",
      "title": "Get Requests",
