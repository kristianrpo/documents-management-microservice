      "type": "graph",
      "title": "Active Goroutines",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "go_goroutines{job=\"documents-service\"}",
          "refId": "A"
        }
      ],
      "yaxes": [
        {"format": "short", "label": null, "logBase": 1, "max": null, "min": 0, "show": true},
        {"format": "short", "label": null, "logBase": 1, "max": null, "min": null, "show": true}
      ]
    },
    {
      "id": 8,
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 18},
      "type": "graph",
      "title": "Memory Usage (MB)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "go_memstats_heap_inuse_bytes{job=\"documents-service\"} / 1024 / 1024",
          "legendFormat": "Heap In Use",
          "refId": "A"
        },
        {
          "expr": "go_memstats_alloc_bytes{job=\"documents-service\"} / 1024 / 1024",
          "legendFormat": "Allocated",
          "refId": "B"
        }
      ],
      "yaxes": [
        {"format": "mbytes", "label": null, "logBase": 1, "max": null, "min": 0, "show": true},
        {"format": "short", "label": null, "logBase": 1, "max": null, "min": null, "show": true}
      ]
    },
    {
      "id": 13,
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 26},
      "type": "graph",
      "title": "Operation Rates (ops/s)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(documents_service_upload_requests_total[5m])",
          "legendFormat": "Uploads",
          "refId": "A"
        },
        {
          "expr": "rate(documents_service_get_requests_total[5m])",
          "legendFormat": "Get (Single)",
          "refId": "B"
        },
        {
          "expr": "rate(documents_service_list_requests_total[5m])",
          "legendFormat": "List Operations",
          "refId": "C"
        },
        {
          "expr": "rate(documents_service_delete_requests_total[5m])",
          "legendFormat": "Deletes (Single)",
          "refId": "D"
        },
        {
          "expr": "rate(documents_service_delete_bulk_requests_total[5m])",
          "legendFormat": "Bulk Deletes",
          "refId": "E"
        },
        {
          "expr": "rate(documents_service_transfer_requests_total[5m])",
          "legendFormat": "Transfers",
          "refId": "F"
        }
      ],
      "yaxes": [
        {"format": "ops", "label": null, "logBase": 1, "max": null, "min": 0, "show": true},
        {"format": "short", "label": null, "logBase": 1, "max": null, "min": null, "show": true}
      ]
    },
    {
      "id": 14,
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 26},
      "type": "graph",
      "title": "HTTP Status Codes",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(documents_service_http_requests_total{status=~\"2..\"}[5m])",
          "legendFormat": "2xx Success",
          "refId": "A"
        },
        {
          "expr": "rate(documents_service_http_requests_total{status=~\"4..\"}[5m])",
          "legendFormat": "4xx Client Error",
          "refId": "B"
        },
        {
          "expr": "rate(documents_service_http_requests_total{status=~\"5..\"}[5m])",
          "legendFormat": "5xx Server Error",
          "refId": "C"
        }
      ],
      "yaxes": [
        {"format": "reqps", "label": null, "logBase": 1, "max": null, "min": 0, "show": true},
        {"format": "short", "label": null, "logBase": 1, "max": null, "min": null, "show": true}
      ]
    }
  ],
  "time": {"from": "now-15m", "to": "now"},
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
  }
}
